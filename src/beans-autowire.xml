<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
<bean id="address1" class="com.dm.spring.autowire.Address" p:city="shagnhai" p:street="lujiazui"></bean>

<bean id="car" class="com.dm.spring.autowire.Car" p:brand="benchi" p:price="800000"></bean>

<bean id="person" class="com.dm.spring.autowire.Person" p:name="Lideming" autowire="byType"></bean>

</beans>
